{
  "name": "@clawshield/remotion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "remotion studio",
    "render": "remotion render src/index.ts ClawShieldDemo ../media/video/clawshield-demo.mp4 --codec=h264",
    "still": "remotion still src/index.ts ClawShieldCover ../media/video/clawshield-cover.png",
    "voiceover": "tsx scripts/generate-voiceover.ts",
    "prepare:v2": "tsx scripts/prepare-assets-v2.ts",
    "sync:v2": "tsx scripts/sync-video-v2-data.ts",
    "voiceover:gemini": "tsx scripts/generate-voiceover-gemini.ts",
    "subtitles:v2": "tsx scripts/generate-subtitles-v2.ts",
    "render:v2": "npm run prepare:v2 && npm run sync:v2 && npm run voiceover:gemini && npm run subtitles:v2 && remotion render src/index.ts ClawShieldDemoV2 ../media/video/clawshield-demo-v2.mp4 --codec=h264",
    "still:v2": "remotion still src/index.ts ClawShieldCoverV2 ../media/video/clawshield-cover-v2.png",
    "prepare:v3": "tsx scripts/prepare-assets-v3.ts",
    "sync:v3": "tsx scripts/sync-video-v3-data.ts",
    "voiceover:gemini:v3": "tsx scripts/generate-voiceover-gemini-v3.ts",
    "subtitles:v3": "tsx scripts/generate-subtitles-v3.ts",
    "render:v3": "npm run prepare:v3 && npm run sync:v3 && npm run voiceover:gemini:v3 && npm run subtitles:v3 && remotion render src/index.ts ClawShieldDemoV3 ../media/video/clawshield-demo-v3.mp4 --codec=h264",
    "render:demo:v3": "remotion render src/index.ts ClawShieldDemoV3 ../media/video/clawshield-demo-v3.mp4 --codec=h264",
    "render:demo:v3:silent": "remotion render src/index.ts ClawShieldDemoV3Silent ../media/video/clawshield-demo-v3-silent.mp4 --codec=h264",
    "still:v3": "remotion still src/index.ts ClawShieldCoverV3 ../media/video/clawshield-cover-v3.png",
    "render:v4": "npm run prepare:v3 && npm run sync:v3 && npm run voiceover:gemini:v3 && npm run subtitles:v3 && remotion render src/index.ts ClawShieldDemoV4 ../media/video/clawshield-demo-v4.mp4 --codec=h264",
    "render:demo:v4": "remotion render src/index.ts ClawShieldDemoV4 ../media/video/clawshield-demo-v4.mp4 --codec=h264",
    "still:v4": "remotion still src/index.ts ClawShieldCoverV4 ../media/video/clawshield-cover-v4.png"
  },
  "dependencies": {
    "@remotion/cli": "^4.0.366",
    "@remotion/google-fonts": "^4.0.366",
    "@remotion/transitions": "^4.0.366",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remotion": "^4.0.366",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
